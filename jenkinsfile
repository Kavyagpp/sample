pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'building...'
                bat 'echo building'
            }
        }
        stage('Test') {
            steps {
                echo 'testing'
                bat 'echo running'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying'
                bat 'echo deploying'
            }
        }
    }
}
